{
  "name": "spren-ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target build --all --parallel=1",
    "e2e": "nx run-many --target e2e --all --parallel=1",
    "test": "nx run-many --target test --all",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --parallel",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:e2e:watch": "nx affected:e2e --watch",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "dep-graph:watch": "nx dep-graph --watch",
    "release": "changeset version",
    "pub:beta": "cd packages/components && pnpm pub:beta",
    "pub:release": "cd packages/components && pnpm pub:release",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config core.hookspath .githooks"
  },
  "author": {
    "name": "Saul Moro",
    "url": "https://twitter.com/SaulMoroDev"
  },
  "homepage": "https://github.com/saulmoro/spren-ui",
  "keywords": [
    "angular",
    "components",
    "headless",
    "ui",
    "spren"
  ],
  "bugs": {
    "url": "https://github.com/saulmoro/spren-ui/issues"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.6.5",
  "dependencies": {
    "@angular/animations": "~16.1.2",
    "@angular/common": "~16.1.2",
    "@angular/compiler": "~16.1.2",
    "@angular/core": "~16.1.2",
    "@angular/forms": "~16.1.2",
    "@angular/platform-browser": "~16.1.2",
    "@angular/platform-browser-dynamic": "~16.1.2",
    "@angular/router": "~16.1.2",
    "rxjs": "~7.8.1",
    "tslib": "~2.5.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.1.1",
    "@angular-devkit/core": "~16.1.1",
    "@angular-devkit/schematics": "~16.1.1",
    "@angular-eslint/eslint-plugin": "~16.0.3",
    "@angular-eslint/eslint-plugin-template": "~16.0.3",
    "@angular-eslint/template-parser": "~16.0.3",
    "@angular/cli": "~16.1.1",
    "@angular/compiler-cli": "~16.1.2",
    "@angular/language-service": "~16.1.2",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@nx/angular": "16.4.0",
    "@nx/cypress": "16.4.0",
    "@nx/eslint-plugin": "16.4.0",
    "@nx/jest": "16.4.0",
    "@nx/js": "16.4.0",
    "@nx/linter": "16.4.0",
    "@nx/web": "16.4.0",
    "@nx/workspace": "16.4.0",
    "@schematics/angular": "~16.1.1",
    "@testing-library/angular": "^14.1.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "~29.5.2",
    "@types/node": "~16.11.68",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "all-contributors-cli": "^6.26.0",
    "cypress": "^12.16.0",
    "eslint": "~8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "jest": "~29.5.0",
    "jest-environment-jsdom": "~29.5.0",
    "jest-preset-angular": "~13.1.1",
    "lint-staged": "^13.2.2",
    "nx": "16.4.0",
    "nx-cloud": "latest",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "ts-jest": "~29.1.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html,component.html}": "prettier --write --ignore-unknown",
    "*.json": "nx workspace-lint"
  }
}
