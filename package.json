{
  "name": "spren-ui",
  "version": "0.0.1",
  "description": "Angular headless components.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nx serve spren-ui -o",
    "build:components": "nx run components:build",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:e2e:watch": "nx affected:e2e --watch",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "dep-graph:watch": "nx dep-graph --watch",
    "release": "changeset version",
    "pub:beta": "pnpm build:components && cd packages/components && pnpm pub:beta",
    "pub:release": "pnpm build:components && cd packages/components && pnpm pub:release",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config core.hookspath .githooks"
  },
  "author": {
    "name": "Saul Moro",
    "url": "https://twitter.com/SaulMoroDev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spren-ui/spren-ui.git"
  },
  "bugs": {
    "url": "https://github.com/spren-ui/spren-ui/issues"
  },
  "homepage": "https://github.com/spren-ui/spren-ui#readme",
  "keywords": [
    "angular",
    "components",
    "headless",
    "spren-ui",
    "spren",
    "ui"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.6.5",
  "dependencies": {
    "@analogjs/content": "~0.2.0-beta.23",
